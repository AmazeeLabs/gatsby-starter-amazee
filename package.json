{
  "name": "gatsby-starter-amazee",
  "private": true,
  "description": "Gatsby starter for Amazee Labs projects",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "commit": "git-cz",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "mock:api": "ts-node src/graphql/server.ts",
    "mocks": "nodemon -e ts ./node_modules/.bin/concurrently npm:mock:*",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "posttest": "yarn typecheck && yarn format:check && yarn lint",
    "tailwind": "tailwind build src/assets/styles/tailwind.css > src/assets/styles/tailwind.build.css",
    "cypress:open": "cypress open",
    "cypress:open:api": "CYPRESS_API_URL=http://localhost:4001 CYPRESS_TEST_LEVEL=api CYPRESS_TAGS='not @UI' cypress open",
    "cypress:run": "./cypress-tests",
    "cypress:run:api": "CYPRESS_API_URL=http://localhost:4001 ./cypress-tests api",
    "storybook": "NODE_ENV=production start-storybook -p 6006 --ci -s public",
    "storybook:build": "NODE_ENV=production build-storybook -s public"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@reach/router": "^1.3.3",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-server": "~2.14.2",
    "apollo-utilities": "^1.3.4",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.23.0",
    "gatsby-plugin-postcss": "^2.3.3",
    "gatsby-plugin-tsconfig-paths": "^1.0.2",
    "gatsby-source-graphql": "^2.5.3",
    "graphql": "^14.6.0",
    "i18next": "^19.0.2",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^2.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.6",
    "react-i18next": "^11.2.6",
    "react-refresh": "^0.8.3",
    "start-server-and-test": "^1.10.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-a11y": "^5.3.17",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/core": "^5.3.17",
    "@storybook/preset-typescript": "^1.2.2",
    "@storybook/react": "^5.3.17",
    "@storybook/source-loader": "^5.3.17",
    "@testing-library/cypress": "^6.0.0",
    "@types/chai": "^4.2.10",
    "@types/classnames": "^2.2.9",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/webpack": "^4.41.7",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "chai": "^4.2.0",
    "cypress": "^4.7.0",
    "cypress-cucumber-preprocessor": "https://github.com/liamchilds/cypress-cucumber-preprocessor.git#configurable-loc-non-global-steps",
    "cypress-wait-until": "^1.7.1",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fork-ts-checker-webpack-plugin": "^4.1.0",
    "husky": "^4.2.5",
    "lodash": "^4.17.15",
    "mocha": "^7.1.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-is": "^16.12.0",
    "regenerator-runtime": "^0.13.5",
    "semantic-release": "^17.1.1",
    "tailwindcss": "^1.2.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.41.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/gatsby-starter-amazee"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/gatsby-starter-amazee/issues"
  },
  "release": {
    "branches": [
      "development"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
