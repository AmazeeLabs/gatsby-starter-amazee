{
  "name": "gatsby-starter-amazee",
  "private": true,
  "description": "Gatsby starter for Amazee Labs projects",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,tsx,json,md}\"",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "mock-swapi": "ts-node mocks/swapi.ts",
    "mocks": "concurrently npm:mock-*",
    "clean": "gatsby clean",
    "cypress:environment": "CI=true concurrently -s first npm:mock-* \"gatsby develop\"",
    "cypress:test": "start-server-and-test cypress:environment http://localhost:8000 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "jest": "jest",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "tailwind": "tailwind build src/styles/tailwind.css > src/styles/tailwind.build.css"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@cypress/webpack-preprocessor": "latest",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@reach/router": "^1.2.1",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/preset-typescript": "^1.1.0",
    "apollo-boost": "^0.4.4",
    "apollo-server": "^2.9.13",
    "autoprefixer": "^9.7.3",
    "classnames": "^2.2.6",
    "concurrently": "^5.0.0",
    "cypress": "^3.7.0",
    "cypress-cucumber-preprocessor": "^1.18.0",
    "eslint": "^6.7.2",
    "gatsby": "^2.18.4",
    "gatsby-plugin-postcss": "^2.1.16",
    "gatsby-plugin-typescript": "^2.1.20",
    "gatsby-source-graphql": "2.1.25",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "postcss-discard-comments": "^4.0.2",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "start-server-and-test": "^1.10.6",
    "tailwindcss": "^1.1.4",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/chai": "^4.2.6",
    "@types/classnames": "^2.2.9",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.23",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-loader": "^8.0.6",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-is": "^16.12.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmazeeLabs/gatsby-starter-amazee"
  },
  "bugs": {
    "url": "https://github.com/AmazeeLabs/gatsby-starter-amazee/issues"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
